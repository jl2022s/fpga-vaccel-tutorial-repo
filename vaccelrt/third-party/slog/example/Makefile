CFLAGS = -g -O2 -Wall
LIBS = -lslog -lpthread
NAME = example
ODIR = .
OBJ = o

OBJS = example.$(OBJ)
OBJECTS = $(patsubst %,$(ODIR)/%,$(OBJS))

.c.$(OBJ):
	@test -d $(ODIR) || mkdir $(ODIR)
	$(CC) $(CFLAGS) -c -o $(ODIR)/$@ $< $(LIBS)

$(NAME):$(OBJS)
	$(CC) $(CFLAGS) -o $(ODIR)/$(NAME) $(OBJECTS) $(LIBS)

.PHONY: clean
clean:
	$(RM) $(ODIR)/$(NAME) $(OBJECTS)
